{
  "version": 2,
  "outputDirectory": "public",
  "functions": {
    "api/index.ts": {
      "maxDuration": 30,
      "includeFiles": "shared/**"
    }
  },
  "routes": [
    {
      "src": "/api/test",
      "dest": "/api/test.ts"
    },
    {
      "src": "/api/health",
      "dest": "/api/health.ts"
    },
    {
      "src": "/api/minimal",
      "dest": "/api/minimal.ts"
    },
    {
      "src": "/api/products-working",
      "dest": "/api/products-working.ts"
    },
    {
      "src": "/api/products-main",
      "dest": "/api/products-main.ts"
    },
    {
      "src": "/api/products",
      "dest": "/api/products-working.ts"
    },
    {
      "src": "/api/faqs",
      "dest": "/api/products-working.ts"
    },
    {
      "src": "/api/auth/status",
      "dest": "/api/products-working.ts"
    },
    {
      "src": "/uploads/(.*)",
      "dest": "/client/public/uploads/$1"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot|webp))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}