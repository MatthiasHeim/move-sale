{
  "version": 2,
  "outputDirectory": "public",
  "routes": [
    {
      "src": "/api/test",
      "dest": "/api/test.ts"
    },
    {
      "src": "/api/health",
      "dest": "/api/health.ts"
    },
    {
      "src": "/api/minimal",
      "dest": "/api/minimal.ts"
    },
    {
      "src": "/api/products-working",
      "dest": "/api/products-working.ts"
    },
    {
      "src": "/api/products-main",
      "dest": "/api/products-main.ts"
    },
    {
      "src": "/api/products",
      "dest": "/api/products-working.ts"
    },
    {
      "src": "/api/faqs",
      "dest": "/api/products-working.ts"
    },
    {
      "src": "/api/auth/login",
      "dest": "/api/auth-login.ts"
    },
    {
      "src": "/api/auth/logout",
      "dest": "/api/auth-logout.ts"
    },
    {
      "src": "/api/auth/status",
      "dest": "/api/auth-status.ts"
    },
    {
      "src": "/api/admin/products",
      "dest": "/api/admin-products.ts"
    },
    {
      "src": "/api/upload",
      "dest": "/api/upload.ts"
    },
    {
      "src": "/api/agent/draft",
      "dest": "/api/agent-draft.ts"
    },
    {
      "src": "/uploads/(.*)",
      "dest": "/client/public/uploads/$1"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot|webp))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}